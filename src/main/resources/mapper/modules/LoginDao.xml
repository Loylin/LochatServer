<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loylin.ems.dao.LoginDao">

    <select id="queryLogin" parameterType="java.util.HashMap" resultType="com.loylin.ems.entity.UserInfo">
        <!-- 如何告诉MyBatis把结果映射到Appointment同时映射book属性 -->
		select * from data where id = #{id}
